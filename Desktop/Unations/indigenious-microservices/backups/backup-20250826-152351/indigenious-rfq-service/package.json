{
  "name": "@indigenious/rfq-service",
  "version": "1.0.0",
  "description": "Indigenious Platform - RFQ (Request for Quote) Management Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t rfq-service .",
    "docker:run": "docker run -p 3004:3004 rfq-service",
    "migration:create": "knex migrate:make",
    "migration:run": "knex migrate:latest",
    "migration:rollback": "knex migrate:rollback",
    "seed:run": "knex seed:run"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "axios": "^1.6.2",
    "@elastic/elasticsearch": "^8.10.0",
    "@aws-sdk/client-s3": "^3.454.0",
    "slug": "^8.2.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "cron": "^3.1.6",
    "multer": "^1.4.5-lts.1",
    "knex": "^3.0.1",
    "pdf-lib": "^1.17.1",
    "docx": "^8.5.0",
    "csv-parser": "^3.0.0",
    "fast-csv": "^4.3.6",
    "nodemailer": "^6.9.7",
    "handlebars": "^4.7.8",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.10",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/slug": "^5.0.7",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "keywords": [
    "indigenious",
    "rfq",
    "procurement",
    "bids",
    "government",
    "microservice"
  ],
  "author": "Indigenious Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:IndigeniousCA/indigenious-rfq-service.git"
  }
}