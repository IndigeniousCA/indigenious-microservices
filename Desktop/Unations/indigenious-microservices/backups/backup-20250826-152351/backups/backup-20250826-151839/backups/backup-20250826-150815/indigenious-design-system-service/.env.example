# Database
DATABASE_URL=postgresql://postgres:indigenous2024@localhost:5449/indigenious_design_system

# Redis
REDIS_URL=redis://localhost:6379

# Server
NODE_ENV=development
PORT=3049

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Security
JWT_SECRET=your-secret-key-here

# Performance
PERFORMANCE_MODE=full-forest
ANIMATION_FPS_TARGET=60
PARTICLE_COUNT_MAX=100
BACKGROUND_LAYERS_MAX=8

# Features
ENABLE_WEBSOCKETS=true
ENABLE_SEASONAL_THEMES=true
ENABLE_CEREMONY_AWARENESS=true
ENABLE_CULTURAL_CALENDAR=true

# Defaults
DEFAULT_LANGUAGE=en
DEFAULT_THEME=auto
ECOSYSTEM_HEALTH_THRESHOLD=80

# Cache
CACHE_TTL=3600

# Logging
LOG_LEVEL=info

# Storage
UPLOAD_PATH=./uploads
ASSETS_PATH=./public/assets

# Design Philosophy
PHILOSOPHY="Where Economics Meets The Land"
ELEMENTAL_COMPONENTS=16
BACKGROUND_LAYERS=8
NATURAL_LAW_COMBINATIONS=5