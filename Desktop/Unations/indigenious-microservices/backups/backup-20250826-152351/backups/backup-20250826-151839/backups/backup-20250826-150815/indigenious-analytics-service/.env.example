# Server Configuration
NODE_ENV=development
PORT=3009

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/analytics_db

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# ClickHouse Configuration (for OLAP analytics)
CLICKHOUSE_URL=http://localhost:8123
CLICKHOUSE_DB=analytics
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# Kafka Configuration (for real-time streaming)
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=analytics-service
ENABLE_KAFKA=true

# Security
JWT_SECRET=your-jwt-secret-here

# Machine Learning Configuration
ENABLE_ML=true
ML_MODEL_PATH=./models
ML_TRAINING_SCHEDULE=0 2 * * * # Daily at 2 AM

# AWS Configuration (for model storage and data export)
AWS_REGION=ca-central-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
S3_BUCKET_NAME=indigenious-analytics-data
S3_MODEL_BUCKET=indigenious-ml-models

# External APIs
# Indigenous Services Canada API
ISC_API_URL=https://api.isc-sac.gc.ca
ISC_API_TOKEN=your-isc-api-token

# CCAB (Canadian Council for Aboriginal Business) API
CCAB_API_URL=https://api.ccab.com
CCAB_API_KEY=your-ccab-api-key

# Statistics Canada API
STATCAN_API_URL=https://www150.statcan.gc.ca/t1/wds/rest
STATCAN_API_KEY=your-statcan-key

# Application URLs
APP_URL=https://analytics.indigenious.ca
FRONTEND_URL=https://app.indigenious.ca
DASHBOARD_URL=https://dashboard.indigenious.ca

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=200

# Caching
CACHE_TTL_SHORT=300      # 5 minutes
CACHE_TTL_MEDIUM=1800    # 30 minutes
CACHE_TTL_LONG=3600      # 1 hour
CACHE_TTL_REPORTS=86400  # 24 hours

# Data Processing
BATCH_SIZE=1000
MAX_PARALLEL_JOBS=5
AGGREGATION_SCHEDULE=0 */6 * * * # Every 6 hours

# Data Quality
DATA_QUALITY_THRESHOLD=80
ANOMALY_DETECTION_SENSITIVITY=2.5
ALERT_THRESHOLD_HIGH=0.8
ALERT_THRESHOLD_MEDIUM=0.6

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/analytics.log
LOG_MAX_SIZE=100M
LOG_MAX_FILES=10

# Monitoring
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30
ENABLE_PROMETHEUS=true

# Feature Flags
ENABLE_PREDICTIVE_ANALYTICS=true
ENABLE_REAL_TIME_DASHBOARD=true
ENABLE_INDIGENOUS_ANALYTICS=true
ENABLE_AUTOMATED_REPORTS=true
ENABLE_DATA_EXPORT=true

# Development/Testing
MOCK_EXTERNAL_APIS=false
SEED_TEST_DATA=false
DEBUG_ML_TRAINING=false