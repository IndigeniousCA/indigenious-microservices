{
  "name": "@indigenious/compliance-service",
  "version": "1.0.0",
  "description": "Compliance and certification service for Indigenous Procurement Platform - manages business certifications, regulatory compliance, and audit trails",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "audit": "npm audit --production"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "ioredis": "^5.3.2",
    "bull": "^4.11.5",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/client-textract": "^3.478.0",
    "@aws-sdk/client-rekognition": "^3.478.0",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "dayjs": "^1.11.10",
    "uuid": "^9.0.1",
    "crypto-js": "^4.2.0",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "xlsx": "^0.18.5",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "@types/crypto-js": "^4.2.1",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@faker-js/faker": "^8.3.1",
    "prisma": "^5.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IndigeniousCA/indigenious-compliance-service.git"
  },
  "keywords": [
    "compliance",
    "certification",
    "audit",
    "regulatory",
    "indigenous",
    "verification",
